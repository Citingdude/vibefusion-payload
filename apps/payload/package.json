{
  "name": "@repo/payload",
  "type": "module",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev --port 4000",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "check-types": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "ci": "payload migrate && pnpm build"
  },
  "dependencies": {
    "@orpc/client": "catalog:",
    "@orpc/server": "catalog:",
    "@payloadcms/db-postgres": "catalog:",
    "@payloadcms/next": "catalog:",
    "@payloadcms/payload-cloud": "catalog:",
    "@payloadcms/plugin-nested-docs": "catalog:",
    "@payloadcms/plugin-seo": "catalog:",
    "@payloadcms/richtext-lexical": "catalog:",
    "@payloadcms/ui": "catalog:",
    "@ts-rest/core": "3.52.1",
    "@ts-rest/next": "3.52.1",
    "@ts-rest/serverless": "3.52.1",
    "cross-env": "7.0.3",
    "defu": "6.1.4",
    "next": "15.2.5",
    "payload": "catalog:",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sharp": "0.34.1",
    "slugify": "1.6.6",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:",
    "@types/node": "22.14.0",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "eslint": "catalog:",
    "eslint-config-next": "15.2.5",
    "typescript": "catalog:"
  }
}
