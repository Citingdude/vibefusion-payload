{
  "name": "@repo/payload",
  "type": "module",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev --port 4000",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "check-types": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "ci": "payload migrate && pnpm build"
  },
  "dependencies": {
    "@orpc/client": "catalog:orpc",
    "@orpc/server": "catalog:orpc",
    "@payloadcms/db-postgres": "catalog:payload",
    "@payloadcms/next": "catalog:payload",
    "@payloadcms/payload-cloud": "catalog:payload",
    "@payloadcms/plugin-nested-docs": "catalog:payload",
    "@payloadcms/plugin-seo": "catalog:payload",
    "@payloadcms/richtext-lexical": "catalog:payload",
    "@payloadcms/ui": "catalog:payload",
    "cross-env": "7.0.3",
    "defu": "6.1.4",
    "next": "catalog:next",
    "payload": "catalog:payload",
    "react": "catalog:next",
    "react-dom": "catalog:next",
    "sharp": "0.34.3",
    "slugify": "1.6.6",
    "zod": "catalog:types"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:lint",
    "@types/node": "catalog:next",
    "@types/react": "catalog:next",
    "@types/react-dom": "catalog:next",
    "eslint": "catalog:lint",
    "eslint-config-next": "catalog:lint",
    "typescript": "catalog:types"
  }
}
